<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane stylesheets="@../stylesheet.css" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="cz.naseLekarna.gui.OrderListController">
    <children>
        <VBox alignment="TOP_CENTER">
            <children>
                <ScrollPane fitToWidth="true" hbarPolicy="NEVER" style="-fx-background-color: white;"
                            VBox.vgrow="ALWAYS">
                    <content>
                        <VBox fx:id="activeOrders"/>
                    </content>
                </ScrollPane>
            </children>
        </VBox>
    </children>
</StackPane>
