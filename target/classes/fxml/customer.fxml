<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox stylesheets="@../stylesheet.css" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/null"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="cz.naseLekarna.gui.lists.SavedCustomerController">
    <children>
        <GridPane onMouseClicked="#writeData" onTouchPressed="#writeDataToo"
                  style="-fx-background-color: #d3d3d3; -fx-background-radius: 10;">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label fx:id="name" GridPane.halignment="CENTER" GridPane.rowSpan="2147483647"
                       GridPane.valignment="CENTER">
                    <font>
                        <Font name="Avenir Book" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="phoneNumber" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                       GridPane.valignment="CENTER">
                    <font>
                        <Font name="Avenir Book" size="13.0"/>
                    </font>
                </Label>
                <Label fx:id="address" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                       GridPane.valignment="CENTER">
                    <font>
                        <Font name="Avenir Book" size="13.0"/>
                    </font>
                </Label>
            </children>
        </GridPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="20.0" right="20.0"/>
    </padding>
</VBox>
