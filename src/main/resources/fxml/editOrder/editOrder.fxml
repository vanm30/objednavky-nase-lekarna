<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox alignment="CENTER_LEFT" stylesheets="@/stylesheet.css" xmlns="http://javafx.com/javafx/null"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.naseLekarna.gui.editOrder.EditInfoController">
    <children>
        <VBox style="-fx-background-color: #d3d3d3; -fx-background-radius: 10;">
            <children>
                <HBox>
                    <children>
                        <VBox>
                            <children>
                                <Label prefHeight="29.0" text="Číslo lístku">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                </Label>
                                <Label prefHeight="29.0" text="Klient">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <Label prefHeight="29.0" text="Telefon">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <Label prefHeight="29.0" text="Ulice">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <Label prefHeight="29.0" text="Obec">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <Label prefHeight="29.0" text="Datum přimu">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <Label prefHeight="29.0" text="Způsob vyzvednutí">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <Label prefHeight="29.0" text="Datum výdeje">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </Label>
                            </children>
                        </VBox>
                        <Region HBox.hgrow="ALWAYS"/>
                        <VBox alignment="TOP_CENTER" fillWidth="false" HBox.hgrow="NEVER">
                            <children>
                                <TextField fx:id="orderNumber" alignment="CENTER" prefHeight="0.0" prefWidth="65.0"
                                           style="-fx-border-color: transparent; -fx-border-radius: 10;-fx-background-radius: 10"
                                           VBox.vgrow="NEVER">
                                    <font>
                                        <Font name="Avenir Book" size="13.0"/>
                                    </font>
                                </TextField>
                                <TextField fx:id="name" prefHeight="20.0" prefWidth="130.0" promptText="Nevyplněno"
                                           style="-fx-border-color: transparent; -fx-border-radius: 10;-fx-background-radius: 10">
                                    <font>
                                        <Font name="Avenir Book" size="12.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <TextField fx:id="phoneNumber" prefHeight="20.0" prefWidth="130.0"
                                           promptText="Nevyplněno"
                                           style="-fx-border-color: transparent; -fx-border-radius: 10;-fx-background-radius: 10">
                                    <font>
                                        <Font name="Avenir Book" size="12.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <TextField fx:id="street" prefHeight="20.0" prefWidth="130.0" promptText="Nevyplněno"
                                           style="-fx-border-color: transparent; -fx-border-radius: 10;-fx-background-radius: 10">
                                    <font>
                                        <Font name="Avenir Book" size="12.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <TextField fx:id="city" prefHeight="20.0" prefWidth="130.0" promptText="Nevyplněno"
                                           style="-fx-border-color: transparent; -fx-border-radius: 10;-fx-background-radius: 10">
                                    <font>
                                        <Font name="Avenir Book" size="12.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <DatePicker fx:id="dateBegin" prefHeight="20.0" prefWidth="130.0"
                                            promptText="Datum přijmu" style="-fx-border-color: transparent">
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </DatePicker>
                                <ChoiceBox fx:id="pickUpOption" prefHeight="20.0" prefWidth="130.0"
                                           style="-fx-border-color: transparent">
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </ChoiceBox>
                                <DatePicker fx:id="dateEnd" prefHeight="20.0" prefWidth="130.0"
                                            promptText="Datum ukončení" style="-fx-border-color: transparent">
                                    <VBox.margin>
                                        <Insets top="5.0"/>
                                    </VBox.margin>
                                </DatePicker>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
            <VBox.margin>
                <Insets left="20.0" right="20.0"/>
            </VBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
        <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity">
            <children>
                <Region HBox.hgrow="ALWAYS"/>
                <Button alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                        minWidth="-Infinity" mnemonicParsing="false" onAction="#editItems" prefHeight="25.0"
                        prefWidth="80.0"
                        style="-fx-background-color: #5ead87#5ead87 #5ead87#5ead87; -fx-background-radius: 5;"
                        text="POLOŽKY" textFill="WHITE">
                    <font>
                        <Font name="Avenir Next Bold" size="12.0"/>
                    </font>
                    <padding>
                        <Insets left="5.0" right="5.0"/>
                    </padding>
                </Button>
                <Region HBox.hgrow="ALWAYS"/>
            </children>
            <VBox.margin>
                <Insets top="10.0"/>
            </VBox.margin>
        </HBox>
        <Label text="Poznámka" textFill="#5ead87">
            <font>
                <Font name="Avenir Next Bold" size="12.0"/>
            </font>
            <VBox.margin>
                <Insets left="25.0"/>
            </VBox.margin>
        </Label>
        <VBox style="-fx-background-color: #d3d3d3 #d3d3d3; -fx-background-radius: 10;">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <VBox.margin>
                <Insets left="20.0" right="20.0"/>
            </VBox.margin>
            <children>
                <TextArea fx:id="notes" style="-fx-background-color: white;">
                    <font>
                        <Font name="Avenir Book" size="12.0"/>
                    </font>
                </TextArea>
            </children>
        </VBox>
        <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity">
            <children>
                <Region HBox.hgrow="ALWAYS"/>
                <Button alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                        minWidth="-Infinity" mnemonicParsing="false" onAction="#finishTask" prefHeight="25.0"
                        prefWidth="104.0" style="-fx-background-color: #e36f5f; -fx-background-radius: 5;"
                        text="DOKONČENO" textFill="WHITE">
                    <font>
                        <Font name="Avenir Next Bold" size="12.0"/>
                    </font>
                    <padding>
                        <Insets left="5.0" right="5.0"/>
                    </padding>
                </Button>
                <Region HBox.hgrow="ALWAYS"/>
                <Button alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                        minWidth="-Infinity" mnemonicParsing="false" onAction="#saveEdit" prefHeight="25.0"
                        prefWidth="80.0"
                        style="-fx-background-color: #5ead87#5ead87 #5ead87#5ead87; -fx-background-radius: 5;"
                        text="ULOŽIT" textFill="WHITE">
                    <font>
                        <Font name="Avenir Next Bold" size="12.0"/>
                    </font>
                    <padding>
                        <Insets left="5.0" right="5.0"/>
                    </padding>
                </Button>
                <Region HBox.hgrow="ALWAYS"/>
            </children>
            <VBox.margin>
                <Insets bottom="20.0" top="10.0"/>
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets top="20.0"/>
    </padding>
</VBox>
