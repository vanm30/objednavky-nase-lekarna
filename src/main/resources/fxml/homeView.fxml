<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="BOTTOM_RIGHT" style="-fx-background-color: white;" stylesheets="@../stylesheet.css" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.naseLekarna.gui.OrderController">
   <children>
      <ScrollPane style="-fx-background-color: transparent;" VBox.vgrow="ALWAYS"/>
      <Button fx:id="OrderButton" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#newOrder" prefHeight="50.0" prefWidth="50.0" style="-fx-background-radius: 100; -fx-background-color: #5ead87#5ead87 #5ead87#5ead87;" VBox.vgrow="NEVER">
